{
  "name": "bookstore-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "_": "____________________________________________________________________________________",
    "db:init": "npm run db:schema:drop && npm run db:migration:empty && npm run db:migration:generate --name=init && npm run build && npm run db:migration:run",
    "db:restart": "npm run db:schema:drop && npm run db:migration:run",
    "db:migration:empty": "rm -rf src/db/migrations",
    "db:migration:generate": "npx typeorm migration:generate src/db/migrations/$npm_config_name -d ./dist/config/orm.config.js",
    "db:migration:create": "npx typeorm migration:create src/db/migrations/$npm_config_name",
    "db:migration:run": "npx typeorm migration:run -d ./dist/config/orm.config.js",
    "db:migration:revert": "npx typeorm migration:revert -d ./dist/config/orm.config.js",
    "db:schema:sync": "npx typeorm schema:sync -d ./dist/config/orm.config.js",
    "db:schema:drop": "npx typeorm schema:drop -d ./dist/config/orm.config.js",
    "__": "___________________________________________________________________________________",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "___": "__________________________________________________________________________________",
    "clean": "npm run format && npm run lint",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@json2csv/plainjs": "^6.1.2",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^2.2.1",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/throttler": "^4.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "cheerio": "^1.0.0-rc.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "csv-parser": "^3.0.0",
    "jsonwebtoken": "^9.0.0",
    "kavenegar": "^1.1.4",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.11",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "18.11.18",
    "@types/node-fetch": "^2.6.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.3.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
